<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="view_controller.ReportsController"
            prefHeight="275.0" prefWidth="375.0" >
    <children>
        <Label fx:id="lblReports" layoutX="25.0" layoutY="20.0" text="Reports Menu">
            <font>
                <Font name="System Bold" size="20.0" />
            </font>
        </Label>

        <Label fx:id="typesByMonthLabel" layoutX="25.0" layoutY="75.0"
               text="Appointment Types by Month" />
        <Button fx:id="typesByMonthBtn" layoutX="250.0" layoutY="71.0" mnemonicParsing="false" prefHeight="30.0"
                prefWidth="100.0"
                text="Generate" />

        <Label fx:id="consultantSchedLabel" layoutX="25.0" layoutY="125.0" text="Schedule for Each Consultant" />
        <Button fx:id="consultantSchedBtn" layoutX="250.0" layoutY="121.0" mnemonicParsing="false" prefHeight="30.0"
                prefWidth="100.0"
                text="Generate" />

        <Label fx:id="custSchedLabel" layoutX="25.0" layoutY="175.0" text="Customer Schedule" />
        <Button fx:id="custSchedBtn" layoutX="250.0" layoutY="171.0" mnemonicParsing="false" prefHeight="30.0"
                prefWidth="100.0"
                text="Generate" />

        <Button fx:id="reportsMainMenuBtn" layoutX="125.0" layoutY="225.0" mnemonicParsing="false" prefHeight="30.0"
                prefWidth="100.0" text="Main Menu" />
        <Button fx:id="reportsExitBtn" layoutX="250.0" layoutY="225.0" mnemonicParsing="false" prefHeight="30.0"
                prefWidth="100.0" text="Exit" />
    </children>
</AnchorPane>
