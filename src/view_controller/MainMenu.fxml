<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="view_controller.MainMenuController"
            prefHeight="350.0" prefWidth="400.0">
    <children>
        <Label layoutX="140.0" layoutY="30.0" text="Main Menu">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <Button fx:id="goToCustBtn" layoutX="100.0" layoutY="100.0" mnemonicParsing="false" prefWidth="200.0"
                text="Customers" />
        <Button fx:id="goToApptBtn" layoutX="100.0" layoutY="160.0" mnemonicParsing="false" prefWidth="200.0"
                text="Appointments" />
        <Button fx:id="goToReportsBtn" layoutX="100.0" layoutY="220.0" mnemonicParsing="false" prefWidth="200.0"
                text="Reports" />
        <Button fx:id="mainMenuExitBtn" layoutX="100.0" layoutY="280.0" mnemonicParsing="false" prefWidth="200.0"
                text="Exit" />
    </children>
</AnchorPane>

